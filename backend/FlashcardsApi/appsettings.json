{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDB": {
    "ConnectionString": "mongodb://USERNAME:PASSWORD@10.0.0.9:27017",
    "Database": "flashcards"
  },
  "Storage": {
    "Provider": "Mongo"
  },
  "Jwt": {
    "Key": "REPLACE_WITH_A_SECRET_KEY",
    "Issuer": "FlashcardsApi",
    "Audience": "FlashcardsApiUsers",
    "ExpireMinutes": 60
  }
}